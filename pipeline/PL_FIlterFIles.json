										"Filename": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ds_jsonoutput",
									"type": "DatasetReference",
									"parameters": {
										"Sinkjsonfiles": {
											"value": "@item().name",
											"type": "Expression"
										}
									}
								}
